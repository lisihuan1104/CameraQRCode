<Window x:Class="CameraScanQRCode.ScanQRCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CameraScanQRCode"
        mc:Ignorable="d"
        Title="ScanQRCode" Height="350" Width="350" Loaded="Window_Loaded" Closed="Window_Closed">
    <Grid>
        <Grid Width="300" Height="300" Background="DimGray">
            <Image Stretch="Fill" Name="imagePlay"></Image>
            <Image Source="Image\Scan.png" Width="300" Height="12" Margin="0,-300,0,0" Name="imgScan"></Image>
            <Ellipse Width="30" Height="30" Fill="YellowGreen" Name="epStatus" Visibility="Collapsed"></Ellipse>
        </Grid>
    </Grid>
</Window>
